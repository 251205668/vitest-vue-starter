{
  "private": true,
  "packageManager": "pnpm@6.32.3",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint --ext .js,.jsx,.vue,.ts,.tsx src",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^8.0.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.8",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@iconify-json/carbon": "^1.1.1",
    "@types/node": "^17.0.21",
    "@unocss/reset": "^0.27.6",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^8.10.0",
    "husky": ">=6",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "pnpm": "^6.32.3",
    "typescript": "^4.6.2",
    "unocss": "^0.27.6",
    "unplugin-auto-import": "^0.6.4",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.8.6",
    "vitest": "^0.6.0",
    "vue-tsc": "^0.32.1"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "no-console": "off"
    }
  },
  "lint-staged": {
    "*.{vue,ts,tsx}": [
      "npm run lint"
    ]
  }
}
